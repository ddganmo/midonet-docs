...
core_plugin = <fo:inline font-weight="bold">midonet_v2_ext</fo:inline>
service_plugins = <fo:inline font-weight="bold">midonet_l3_ext</fo:inline>
...
dhcp_agent_notification = False
...
allow_overlapping_ips = True
...
rpc_backend = rabbit
...
auth_strategy = keystone
...
